						"urlOff": "https://dweet.io:443/dweet/for/LHLAXN?SWITCH1=0&SWITCH2=0",
						"on_text": "switch 2 = ON",
						"off_text": "switch 2 = OFF"
					}
				},
				{
					"type": "slider_plugin",
					"settings": {
						"title": "Setpoint ROOM 233",
						"min": "22",
						"max": "27",
						"value": "datasources[\"SLIDER233\"]",
						"url": "https://dweet.io:443/dweet/for/LHLAXN?SLIDER233=%VALUE%"
					}
				},
				{
					"type": "gauge",
					"settings": {
						"title": "Temperature",
						"value": "datasources[\"LH-RT-N\"][\"Ambient\"]",
						"units": "deg C",
						"min_value": 0,
						"max_value": "50"
					}
				}
			]
		},
		{
